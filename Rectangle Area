#include <iostream>

using namespace std;
/*
 * Create classes Rectangle and RectangleArea
 */
class Rectangle
{
  protected:
  int width;
  int height;
    public:
    void read_input()
    {
        cin>>width>>height;
    }
     void display()
    {
    cout<<width<<" "<<height<<"\n";
    } 
};
class RectangleArea :public Rectangle
{
    public:
      void display()
    {
      cout<<width * height;
    }
}; 


int main()
{
   
    r_area.display();
    
    return 0;
